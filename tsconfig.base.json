{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@angular-slack/auth/data-access": ["libs/auth/data-access/src/index.ts"],
      "@angular-slack/auth/feature-auth": [
        "libs/auth/feature-auth/src/index.ts"
      ],
      "@angular-slack/chat-message": ["libs/workspace/ui-message/src/index.ts"],
      "@angular-slack/client/data-access": [
        "libs/client/data-access/src/index.ts"
      ],
      "@angular-slack/data-access-channels": [
        "libs/workspace/data-access-channels/src/index.ts"
      ],
      "@angular-slack/data-access-contacts": [
        "libs/workspace/data-access-contacts/src/index.ts"
      ],
      "@angular-slack/data-access-messages": [
        "libs/workspace/data-access-messages/src/index.ts"
      ],
      "@angular-slack/feature-header-toolbar": [
        "libs/header-toolbar/feature-header-toolbar/src/index.ts"
      ],
      "@angular-slack/feature-shell": [
        "libs/client/feature-shell/src/index.ts"
      ],
      "@angular-slack/file-preview": [
        "libs/workspace/ui-file-preview/src/index.ts"
      ],
      "@angular-slack/message-editor": [
        "libs/workspace/feature-message-editor/src/index.ts"
      ],
      "@angular-slack/slack-api": [
        "libs/shared/data-access/slack-api/src/index.ts"
      ],
      "@angular-slack/tab-rail/feature-tab-rail": [
        "libs/tab-rail/feature-tab-rail/src/index.ts"
      ],
      "@angular-slack/workspace/feature-primary-view": [
        "libs/workspace/feature-primary-view/src/index.ts"
      ],
      "@angular-slack/workspace/feature-workspace": [
        "libs/workspace/feature-workspace/src/index.ts"
      ],
      "@env/*": ["apps/angular-slack/src/environments/*"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
